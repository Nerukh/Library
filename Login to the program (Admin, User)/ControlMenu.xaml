        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login_to_the_program__Admin__User_"
        mc:Ignorable="d"
        Title="ControlMenu" Height="200" Width="350">
    <Grid>
        <StackPanel Margin="10">
            <Button Name="editingDataUsersButton"  Background="CornflowerBlue" Content="Editing data users"  Margin="0,40,0,0" Click="editingDataUsersButton_Click"/>
            <Button Name="rightsEditorButton"  Background="BlanchedAlmond" Content="Rights editor "  Margin="0,20,0,0" Click="rightsEditorButton_Click" />
        </StackPanel>
    </Grid>
</Window>
